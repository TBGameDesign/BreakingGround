          <argument>
            <kind>1</kind>
            <string>/// Growth over time

if ( waterlevel[curr_waterlevel] &gt; size ) {
size+=1;
}
if ( waterlevel[curr_waterlevel] &lt; size ) {
size-=1;
}

image_index=size;
