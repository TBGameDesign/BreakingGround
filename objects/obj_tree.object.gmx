        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Always check for growth


// the droplets of water from bucket can grow the tree too, but not by much 1/20th
if ( increment &gt;= 1 &amp;&amp; obj_player.fin=0 ) {
    curr_waterlevel+=2;
    increment=0;
}

// based on curr_waterlevel, which effects size, raise the shrinkage to accomodate amount if water

if ( curr_waterlevel &gt;= 0 &amp;&amp; curr_waterlevel &lt; 10) {
    shrink=1
}
if ( curr_waterlevel &gt;= 10 &amp;&amp; curr_waterlevel &lt; 60) {
    shrink=2
}
if ( curr_waterlevel &gt;= 60 &amp;&amp; curr_waterlevel &lt; 140) {
    shrink=3
}
if ( curr_waterlevel &gt;= 140 &amp;&amp; curr_waterlevel &lt; 250) {
    shrink=4
}
if ( curr_waterlevel &gt;= 250 &amp;&amp; curr_waterlevel &lt;= 360) {
    shrink=5
}

if ( waterlevel[curr_waterlevel] &gt; size ) { // if there is enough water to grow
    if ( !alarm[0] ) {
        alarm[0]=30 // Dont want to grow instantly, grow in one second
        alarm[11]=630 // Reset Shrinkage Counter ( added 30 to compensate for growth time )
        }
}

if ( waterlevel[curr_waterlevel] &lt; size ) {
        alarm[0]=1 // Shrink Immediatly
}

if ( curr_waterlevel &gt;= 315 ) {
    obj_player.fin=1
}

image_index=size;
</string>
