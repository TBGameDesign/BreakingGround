        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Tree Shrinking 
// Every 10 seconds reduce tree waterlevel by 1, then reset alarm
if ( curr_waterlevel != 0 &amp;&amp; size != 0 ) { // Dont shrink if your already shrunk!
curr_waterlevel-=shrink; // Shrink factor Grows over time based on size
}
alarm[11]=600 // Shrink again in 20 seconds
</string>
